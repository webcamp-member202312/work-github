<div class="container-fluid">
  <div class="row">
    <div class="col-1"></div>
    <h2 class="mt-4 mb-4">ジャンル編集</h2>
  </div>
  <%= form_with model: [:admin,@genre] do |f| %>
    <div class="row">
      <div class="ml-5 mr-5">
        <strong>ジャンル名</strong>
      </div>
      <%= f.text_field :name, :class => "mr-5" %>
      <%= f.submit "変更を保存", :class => "btn btn-success" %>
    </div>
  <% end %>
</div>